{
  "root": true,
  "env": {
    "node": true
  },
  "extends": ["plugin:vue/vue3-essential", "eslint:recommended", "@vue/prettier"],
  "rules": {
    "prettier/prettier": "off",
    "vue/multi-word-component-names": 0,
    "vue/require-valid-default-prop": 0
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  }
}
